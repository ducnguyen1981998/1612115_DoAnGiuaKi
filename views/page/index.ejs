                    <h3><a href="/detail/<%= i.idquanao%>"><%= i.tenquanao%></a></h3>
                    <p class="price">
                      <del><%= i.gia%></del> <%= i.gia*(100-i.giamgia)/100%>
                    </p>
                  </div>
                  <!-- /.text-->
                  <div class="ribbon sale">
                    <div class="theribbon">-<%= i.giamgia%>%</div>
                    <div class="ribbon-background"></div>
                  </div>
                  <!-- /.ribbon-->
                  <div class="ribbon new">
